public class ArrayUtils{

    public static void populate(int [] arr){
	for (int x: arr){
	    arr[x] = (int) (100.0 * Math.random());
	    System.out.println(x);
	}
    }


    public static void main(String [] args){
	int [] aline = new int [5];
	populate(aline);
	for (int x: aline){
	    System.out.println(x);
	}
    }
}
